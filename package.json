{
  "name": "reactchatbot",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@google-cloud/dialogflow": "^5.7.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/express": "^4.17.17",
    "dialogflow-fulfillment-ts": "^0.6.4",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1",
    "react-scripts": "5.0.1",
    "ts-node-dev": "^2.0.0",
    "web-vitals": "^2.1.4"
  },
  "proxy": "http://localhost:5000/",
  "scripts": {
    "start": "run-p --race start:server start:app",
    "start:app": "react-scripts start",
    "start:server": "cd server && ts-node-dev --respawn --transpile-only ./Server.ts && wait-on tcp:5000",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "tunnel": "ngrok http 3000 --host-header='localhost:3000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "ngrok": "^5.0.0-beta.2",
    "npm-run-all": "^4.1.5"
  }
}
